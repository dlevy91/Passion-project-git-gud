@model gitgudclone.Data.ApplicationDbContext

<title>@ViewData["Title"] GitGud - Tutorials</title>

<div class="card-columns">
    @foreach (PostsModel post in @Model.postsList)
    {
        <a asp-area="" asp-controller="gitgud" asp-action="viewpostdetails" asp-route-postID="@post.id">
            <partial name="_postsDisplay" model="@post" />
        </a>
    }
</div>